<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="networks.SimpleCanvasController">
   <top>
      <ToolBar prefHeight="40.0" BorderPane.alignment="CENTER">
        <items>
            <Button fx:id="alexLike" mnemonicParsing="false" onAction="#showAlexNetwork" text="Alex-Like" />
            <Button fx:id="inceptionLike" mnemonicParsing="false" onAction="#showInceptionNetwork" text="Inception-Like" />
            <Button fx:id="kevinLike" mnemonicParsing="false" onAction="#showKevinNetwork" text="Kevin-Like" />
        </items>
      </ToolBar>
   </top>
   <center>
      <Canvas fx:id="canvas" height="650" width="850" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
